<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.junksellerapp">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.BLUETOOTH" />
    <uses-permission android:name="android.permission.BLUETOOTH_ADMIN" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_recycle"
        android:label="@string/app_name"
        android:networkSecurityConfig="@xml/network_security_config"
        android:roundIcon="@mipmap/ic_launcher_recycle_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity android:name=".week_statistics"></activity>
        <activity android:name=".daily_statistics" />
        <activity android:name=".step.RefillCoin" />
        <activity android:name=".step.step4_carPrice" />
        <activity android:name=".step.step_3_2_movingObject" />
        <activity android:name=".FinalScreen" />
        <activity android:name=".step.step6_DropCoin" />
        <activity android:name=".step.step5_setPrice" />
        <activity android:name=".step.step2_monitorObject" />
        <activity android:name=".step.step3_weightCheck" />
        <activity android:name=".step.ReturnObject" />
        <activity android:name=".HelpScreen" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>